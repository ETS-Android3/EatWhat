<?xml version="1.0" encoding="utf-8"?>

<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/Theme.EatWhat.AppBarOverlay"
        >

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/setPreferenceToolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize" />

    </com.google.android.material.appbar.AppBarLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="75dp"
            android:text="@string/set_preference"
            android:textSize="25sp" />
    </RelativeLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="3"
        android:rowCount="3"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="150dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        >

        <CheckedTextView
            android:id="@+id/asianfusion"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_rowWeight="1"
            android:layout_column="0"
            android:layout_columnWeight="1"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginBottom="10dp"
            android:background="@drawable/round_rectangular"
            android:checked="false"
            android:clickable="true"
            android:minHeight="48dp"
            android:text="asianfusion"
            android:textColor="#978C8C"
            android:textSize="20sp" />

        <CheckedTextView
            android:id="@+id/Tai"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_rowWeight="1"
            android:layout_column="1"
            android:layout_columnWeight="1"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginBottom="10dp"
            android:clickable="true"
            android:background="@drawable/round_rectangular"
            android:text="thai"
            android:textColor="#978C8C"
            android:textSize="20sp" />
        <CheckedTextView
            android:id="@+id/Chinese"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_rowWeight="1"
            android:layout_column="2"
            android:layout_columnWeight="1"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:background="@drawable/round_rectangular"
            android:text="chinese"
            android:clickable="true"
            android:textColor="#978C8C"
            android:textSize="20sp"
            android:layout_marginBottom="10dp" />

        <CheckedTextView
            android:id="@+id/barbeque"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_rowWeight="1"
            android:layout_column="0"
            android:layout_columnWeight="0"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginBottom="10dp"
            android:background="@drawable/round_rectangular"
            android:minHeight="48dp"
            android:text="barbeque"
            android:clickable="true"
            android:textColor="#978C8C"
            android:textSize="20sp" />

        <CheckedTextView
            android:id="@+id/Mexican"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_rowWeight="1"
            android:layout_column="1"
            android:layout_columnWeight="0"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginBottom="10dp"
            android:background="@drawable/round_rectangular"
            android:text="mexican"
            android:clickable="true"
            android:textColor="#978C8C"
            android:textSize="20sp" />
        <CheckedTextView
            android:id="@+id/buffets"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_rowWeight="1"
            android:layout_column="2"
            android:layout_columnWeight="0"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:clickable="true"
            android:background="@drawable/round_rectangular"
            android:text="buffets"
            android:textColor="#978C8C"
            android:textSize="20sp"
            android:layout_marginBottom="10dp" />

        <CheckedTextView
            android:id="@+id/French"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_rowWeight="1"
            android:layout_column="0"
            android:layout_columnWeight="0"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginBottom="10dp"
            android:background="@drawable/round_rectangular"
            android:minHeight="48dp"
            android:text="french"
            android:textColor="#978C8C"
            android:clickable="true"
            android:textSize="20sp" />

        <CheckedTextView

            android:id="@+id/Indian"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_rowWeight="1"
            android:layout_column="1"
            android:layout_columnWeight="0"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:background="@drawable/round_rectangular"
            android:text="indian"
            android:textColor="#978C8C"
            android:textSize="20sp"
            android:clickable="true"
            android:layout_marginBottom="10dp"
            />
        <CheckedTextView

            android:id="@+id/diners"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_rowWeight="1"
            android:layout_column="2"
            android:layout_columnWeight="0"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:background="@drawable/round_rectangular"
            android:text="diners"
            android:clickable="true"
            android:textColor="#978C8C"
            android:textSize="20sp"
            android:layout_marginBottom="10dp" />



    </GridLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="3"
        android:rowCount="3"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="350dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        >

        <CheckedTextView
            android:id="@+id/Korean"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_rowWeight="1"
            android:layout_column="0"
            android:layout_columnWeight="1"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginBottom="10dp"
            android:background="@drawable/round_rectangular"
            android:checked="false"
            android:clickable="true"
            android:minHeight="48dp"
            android:text="korean"
            android:textColor="#978C8C"
            android:textSize="20sp" />

        <CheckedTextView
            android:id="@+id/Japanese"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_rowWeight="1"
            android:layout_column="1"
            android:layout_columnWeight="1"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginBottom="10dp"
            android:clickable="true"
            android:background="@drawable/round_rectangular"
            android:text="japanese"
            android:textColor="#978C8C"
            android:textSize="20sp" />
        <CheckedTextView
            android:id="@+id/brazilian"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_rowWeight="1"
            android:layout_column="2"
            android:layout_columnWeight="1"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:background="@drawable/round_rectangular"
            android:text="brazilian"
            android:clickable="true"
            android:textColor="#978C8C"
            android:textSize="20sp"
            android:layout_marginBottom="10dp" />

        <CheckedTextView
            android:id="@+id/American"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_rowWeight="1"
            android:layout_column="0"
            android:layout_columnWeight="0"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginBottom="10dp"
            android:background="@drawable/round_rectangular"
            android:minHeight="48dp"
            android:text="american"
            android:clickable="true"
            android:textColor="#978C8C"
            android:textSize="20sp" />

        <CheckedTextView
            android:id="@+id/creperies"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_rowWeight="1"
            android:layout_column="1"
            android:layout_columnWeight="0"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginBottom="10dp"
            android:background="@drawable/round_rectangular"
            android:text="creperies"
            android:clickable="true"
            android:textColor="#978C8C"
            android:textSize="20sp" />
        <CheckedTextView
            android:id="@+id/Italian"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_rowWeight="1"
            android:layout_column="2"
            android:layout_columnWeight="0"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:clickable="true"
            android:background="@drawable/round_rectangular"
            android:text="italian"
            android:textColor="#978C8C"
            android:textSize="20sp"
            android:layout_marginBottom="10dp" />

        <CheckedTextView
            android:id="@+id/burgers"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_rowWeight="1"
            android:layout_column="0"
            android:layout_columnWeight="0"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginBottom="10dp"
            android:background="@drawable/round_rectangular"
            android:minHeight="48dp"
            android:text="burgers"
            android:textColor="#978C8C"
            android:clickable="true"
            android:textSize="20sp" />

        <CheckedTextView

            android:id="@+id/cafes"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_rowWeight="1"
            android:layout_column="1"
            android:layout_columnWeight="0"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:background="@drawable/round_rectangular"
            android:text="cafes"
            android:textColor="#978C8C"
            android:textSize="20sp"
            android:clickable="true"
            android:layout_marginBottom="10dp"
            />
        <CheckedTextView

            android:id="@+id/dimsum"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_rowWeight="1"
            android:layout_column="2"
            android:layout_columnWeight="0"
            android:layout_gravity="fill_vertical|fill_horizontal"
            android:textAlignment="gravity"
            android:gravity="center_vertical|center_horizontal"
            android:background="@drawable/round_rectangular"
            android:text="dimsum"
            android:clickable="true"
            android:textColor="#978C8C"
            android:textSize="20sp"
            android:layout_marginBottom="10dp" />

    </GridLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center_horizontal"
        android:orientation="vertical">

        <Button
            android:id="@+id/setPreferenceButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="80dp"
            android:clickable="true"
            android:textColor="@color/white"
            android:textSize="18sp" />
    </LinearLayout>

</androidx.coordinatorlayout.widget.CoordinatorLayout>